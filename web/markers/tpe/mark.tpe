#declare std::vector<std::string> vect#
#declare MarkerTest test#
#declare int number#
#declare std::string dat#
<html>
	<head>
		<script src="${dat}"></script>
		${dat}
		${test.getId()}
	</head>
	<body>
		<input type="text"/>
		<input type="submit"/>
		#for(int i=0;i<number;i++)#
			<input type="text" name="input${i}"/>
		#rof#
		#for(int i=0;i<(int)vect.size();i++)#
			<input type="text" name="input${vect.at(i)}"/>
		#rof#
		#if(number==0)#
			<p>No input fields present</p>
		#fi#
		<p><span>${test.getId()}</span><span>${test.getName()}</span></p>
	</body>
</html>
